      <property name="name"><value>查看作業形式</value></property>
      <property name="location"><value>/sakai_assignment_grades/aqzj_zh_TW.html</value></property>
   </bean>  
   <bean id="returnAssignment" class="org.sakaiproject.component.app.help.model.ResourceBean">
