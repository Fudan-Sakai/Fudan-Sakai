<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="scheduleAbout" class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId"><value>arfm</value></property>
		<property name="name"><value>Schedule</value></property>
		<property name="location"><value>/sakai_schedule/arfm.html</value></property>
		<property name="defaultForTool"><value>sakai.schedule</value></property>
	</bean>
	<bean id="scheduleOverview" class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId"><value>aray</value></property>
		<property name="name"><value>Schedule Overview</value></property>
		<property name="location"><value>/sakai_schedule/aray.html</value></property>
	</bean>
	<bean id="scheduleSummary"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>avaz</value>
		</property>
		<property name="name">
			<value>Schedule summary</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/avaz.html</value>
		</property>
		<property name="defaultForTool">
			<value>sakai.summary.calendar</value>
		</property>
	</bean>
	<bean id="addActivity"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqyn</value>
		</property>
		<property name="name">
			<value>Add, edit or delete item</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqyn.html</value>
		</property>
	</bean>
	<bean id="addActivityAttachment"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>argn</value>
		</property>
		<property name="name">
			<value>Add attachment to item</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/argn.html</value>
		</property>
	</bean>
	<bean id="viewSchedule"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqzh</value>
		</property>
		<property name="name">
			<value>View item details</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqzh.html</value>
		</property>
	</bean>

	<bean id="changeScheduleView"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqyp</value>
		</property>
		<property name="name">
			<value>Change view</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqyp.html</value>
		</property>
	</bean>

	<bean id="addCalendarFields"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqzc</value>
		</property>
		<property name="name">
			<value>Add or delete calendar fields</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqzc.html</value>
		</property>
	</bean>

	<bean id="printCalendar"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>arfe</value>
		</property>
		<property name="name">
			<value>Print calendar</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/arfe.html</value>
		</property>
	</bean>

	<bean id="mergeCalendar"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqyl</value>
		</property>
		<property name="name">
			<value>Merge calendars</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqyl.html</value>
		</property>
	</bean>
	
	<bean id="importCalendar"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>argo</value>
		</property>
		<property name="name">
			<value>Import a calendar</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/argo.html</value>
		</property>
	</bean>


	<bean id="org.sakaiproject.api.app.help.TableOfContents"
		class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
		<property name="name">
			<value>root</value>
		</property>
		<property name="categories">
			<list>
				<bean id="scheduleCategory"
					class="org.sakaiproject.component.app.help.model.CategoryBean">
					<property name="name">
						<value>Schedule</value>
					</property>
					<property name="resources">
						<list>
							<ref bean="scheduleAbout" />
							<ref bean="scheduleOverview" />
							<ref bean="scheduleSummary" />
							<ref bean="addActivity" />
							<ref bean="addActivityAttachment" />
							<ref bean="viewSchedule" />
							<ref bean="addCalendarFields" />
							<ref bean="changeScheduleView" />
							<ref bean="printCalendar" />
							<ref bean="mergeCalendar" />
							<ref bean="importCalendar" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>
</beans>
