   <bean id="siteId" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>atla</value></property>
      <property name="name"><value>Finding your siteID</value></property>
      <property name="location"><value>/sakai_resources/atla.html</value></property>
   </bean>
   
   
    <bean id="org.sakaiproject.api.app.help.TableOfContents" 
          class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
      <property name="name"><value>root</value></property>
      <property name="categories">
         <list>
            <bean id="resourceCategory" class="org.sakaiproject.component.app.help.model.CategoryBean">
               <property name="name"><value>Resources</value></property>
               <property name="resources">
                  <list>
                     <ref bean="resourceIndex"/>
                     <ref bean="resourceOverview"/>
                     <ref bean="viewResource"/>
                     <ref bean="itemDetails"/>
                     <ref bean="usingFolders"/>
                     <ref bean="usingFiles"/>
                     <ref bean="usingLinks"/>
                     <ref bean="usingHtml"/>
                     <ref bean="usingText"/>
                     <ref bean="usingCitation"/>
                     <ref bean="makePublic"/>
                     <ref bean="findURL"/>
                     <ref bean="moveResource"/>
                     <ref bean="copyResource"/>
                     <ref bean="renameResource"/>
                     <ref bean="deleteResource"/>
                     <ref bean="sortResource"/>
                     <ref bean="groupFolders"/>
                     <ref bean="changePermissions"/>
                     <ref bean="unHide"/>
                     <ref bean="unsupportedChars"/>
                     <ref bean="webDavOverview"/>
                     <ref bean="winxpAccess"/>
                     <ref bean="macAccess"/>
                     <ref bean="siteId"/>
                  </list>
               </property>
            </bean>
           </list>
         </property>
       </bean>
</beans>
