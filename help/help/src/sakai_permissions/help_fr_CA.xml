<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
   <bean id="permissionsOverview" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arax_fr_CA</value></property>
      <property name="name"><value>Droits d'accès et rôles: Vue d'ensemble</value></property>
      <property name="location"><value>/sakai_permissions/arax_fr_CA.html</value></property>
      <property name="defaultForTool"><value>sakai.permissions</value></property>      
   </bean>
   
   <bean id="siteInfoChangeRoles" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arfb_fr_CA</value></property>
      <property name="name"><value>Modifier les rôles du participant</value></property>
      <property name="location"><value>/sakai_permissions/arfb_fr_CA.html</value></property>
   </bean>
   
   <bean id="worksiteSetupChangeRoles" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardq_fr_CA</value></property>
      <property name="name"><value>Modifier les rôles des participants à partir de la Configuration de Sites</value></property>
      <property name="location"><value>/sakai_permissions/ardq_fr_CA.html</value></property>
   </bean>
   
   <bean id="participantRoles" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arbu_fr_CA</value></property>
      <property name="name"><value>Rôles du participant</value></property>
      <property name="location"><value>/sakai_permissions/arbu_fr_CA.html</value></property>
   </bean>
   
   <bean id="permissionsRolesTools" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>armh_fr_CA</value></property>
      <property name="name"><value>Droits d'accès, rôles et outils</value></property>
      <property name="location"><value>/sakai_permissions/armh_fr_CA.html</value></property>
   </bean>
   
   <bean id="worksiteSetupAddParticipants" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqyo_fr_CA</value></property>
      <property name="name"><value>Ajouter, éditer ou supprimer des participants à partir de la Configuration de Sites</value></property>
      <property name="location"><value>/sakai_permissions/aqyo_fr_CA.html</value></property>
   </bean>
   
   <bean id="addParticipants" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arav_fr_CA</value></property>
      <property name="name"><value>Ajouter, éditer ou supprimer des participants</value></property>
      <property name="location"><value>/sakai_permissions/arav_fr_CA.html</value></property>
   </bean>
   
   <bean id="siteInfoControlAccess" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arci_fr_CA</value></property>
      <property name="name"><value>Contrôler l'accès à votre site</value></property>
      <property name="location"><value>/sakai_permissions/arci_fr_CA.html</value></property>
   </bean>
   
   <bean id="createWorksites" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqzr_fr_CA</value></property>
      <property name="name"><value>Créer/ajouter, éditer ou supprimer des sites</value></property>
      <property name="location"><value>/sakai_permissions/aqzr_fr_CA.html</value></property>
   </bean>
   
   <bean id="worksiteSetupControlAccess" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqzw_fr_CA</value></property>
      <property name="name"><value>Contrôler l'accès à votre site de cours ou de projet à partir de la Configuration de Sites</value></property>
      <property name="location"><value>/sakai_permissions/aqzw_fr_CA.html</value></property>
   </bean>
   
   <bean id="changePermissions" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardk_fr_CA</value></property>
      <property name="name"><value>Modifier les droits d'accès pour un outil dans un site de cours ou de projet</value></property>
      <property name="location"><value>/sakai_permissions/ardk_fr_CA.html</value></property>
   </bean>
   
   <bean id="permissionsRelationships" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arbp_fr_CA</value></property>
      <property name="name"><value>Types, realms, rôles et droits d'accès</value></property>
      <property name="location"><value>/sakai_permissions/arbp_fr_CA.html</value></property>
   </bean>
         
   
    <bean id="org.sakaiproject.api.app.help.TableOfContents" 
          class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
      <property name="name"><value>root</value></property>
      <property name="categories">
         <list>
            <bean id="permissionsCategory" class="org.sakaiproject.component.app.help.model.CategoryBean">
               <property name="name"><value>Permissions et Rôles</value></property>
               <property name="resources">
                  <list>
                     <ref bean="permissionsOverview"/>
                     <ref bean="siteInfoChangeRoles"/>
                     <ref bean="worksiteSetupChangeRoles"/>
										 <ref bean="participantRoles"/>
										 <ref bean="permissionsRolesTools"/>
										 <ref bean="worksiteSetupAddParticipants"/>
										 <ref bean="addParticipants"/>
										 <ref bean="siteInfoControlAccess"/>
										 <ref bean="createWorksites"/>
										 <ref bean="worksiteSetupControlAccess"/>
										 <ref bean="changePermissions"/>
										 <ref bean="permissionsRelationships"/>
                  </list>
               </property>
            </bean>
           </list>
         </property>
       </bean>
</beans>
