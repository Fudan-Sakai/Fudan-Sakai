      <property name="name"><value>Настройки доступа к сайту</value></property>
      <property name="location"><value>/sakai_permissions/arci_ru.html</value></property>
   </bean>
   
   <bean id="createWorksites" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqzr_ru</value></property>
      <property name="name"><value>Создание/добавление, изменение и удаление сайтов</value></property>
      <property name="location"><value>/sakai_permissions/aqzr_ru.html</value></property>
   </bean>
   
   <bean id="worksiteSetupControlAccess" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqzw_ru</value></property>
      <property name="name"><value>Настройки доступа к сайту через "Мои сайты"</value></property>
      <property name="location"><value>/sakai_permissions/aqzw_ru.html</value></property>
   </bean>
   
   <bean id="changePermissions" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardk_ru</value></property>
      <property name="name"><value>Изменение прав доступа в инструменте</value></property>
      <property name="location"><value>/sakai_permissions/ardk_ru.html</value></property>
   </bean>
   
   <bean id="permissionsRelationships" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arbp_ru</value></property>
