      <property name="location"><value>/sakai_permissions/arax_ru.html</value></property>
      <property name="defaultForTool"><value>sakai.permissions</value></property>      
   </bean>
   
   <bean id="siteInfoChangeRoles" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arfb_ru</value></property>
      <property name="name"><value>Изменение ролей участников</value></property>
      <property name="location"><value>/sakai_permissions/arfb_ru.html</value></property>
   </bean>
   
   <bean id="worksiteSetupChangeRoles" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardq_ru</value></property>
      <property name="name"><value>Изменение ролей в "Моих сайтах"</value></property>
      <property name="location"><value>/sakai_permissions/ardq_ru.html</value></property>
   </bean>
   
   <bean id="participantRoles" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arbu_ru</value></property>
      <property name="name"><value>Роли участников</value></property>
      <property name="location"><value>/sakai_permissions/arbu_ru.html</value></property>
   </bean>
   
   <bean id="permissionsRolesTools" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>armh_ru</value></property>
      <property name="name"><value>Права доступа, роли и инструменты</value></property>
      <property name="location"><value>/sakai_permissions/armh_ru.html</value></property>
   </bean>
   
   <bean id="worksiteSetupAddParticipants" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqyo_ru</value></property>
