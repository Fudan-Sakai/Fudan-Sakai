      <property name="location"><value>/sakai_permissions/arbp_ru.html</value></property>
   </bean>
         
   
    <bean id="org.sakaiproject.api.app.help.TableOfContents" 
          class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
      <property name="name"><value>root</value></property>
      <property name="categories">
         <list>
            <bean id="permissionsCategory" class="org.sakaiproject.component.app.help.model.CategoryBean">
               <property name="name"><value>Права доступа и роли</value></property>
               <property name="resources">
                  <list>
                     <ref bean="permissionsOverview"/>
                     <ref bean="siteInfoChangeRoles"/>
                     <ref bean="worksiteSetupChangeRoles"/>
					 <ref bean="participantRoles"/>
					 <ref bean="permissionsRolesTools"/>
					 <ref bean="worksiteSetupAddParticipants"/>
					 <ref bean="addParticipants"/>
					 <ref bean="siteInfoControlAccess"/>
					 <ref bean="createWorksites"/>
					 <ref bean="worksiteSetupControlAccess"/>
					 <ref bean="changePermissions"/>
					 <ref bean="permissionsRelationships"/>
                  </list>
               </property>
            </bean>
           </list>
         </property>
       </bean>
</beans>
