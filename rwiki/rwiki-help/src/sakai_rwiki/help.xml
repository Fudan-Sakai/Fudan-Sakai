<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
  <bean id="wikiContent1" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content1</value>
    </property>
    <property name="name">
      <value>Wiki Overview</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/overview.htm</value>
    </property>
    <property name="defaultForTool"><value>sakai.rwiki</value></property>

  </bean>
  <bean id="wikiContent2" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content2</value>
    </property>
    <property name="name">
      <value>Viewing pages</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/viewing_pages.htm</value>
    </property>
  </bean>
  <bean id="wikiContent3" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content3</value>
    </property>
    <property name="name">
      <value>Editing pages</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/editing_pages.htm</value>
    </property>
  </bean>
  <bean id="wikiContent4" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content4</value>
    </property>
    <property name="name">
      <value>Creating new pages</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/creating_pages.htm</value>
    </property>
  </bean>
  <bean id="wikiContent5" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content5</value>
    </property>
    <property name="name">
      <value>Adding images and attachments</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/adding_images.htm</value>
    </property>
  </bean>
  <bean id="wikiContent6" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content6</value>
    </property>
    <property name="name">
      <value>Viewing page info</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/viewing_info.htm</value>
    </property>
  </bean>
  <bean id="wikiContent7" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content7</value>
    </property>
    <property name="name">
      <value>Viewing page history</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/viewing_history.htm</value>
    </property>
  </bean>
  <bean id="wikiContent8" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>wiki_content8</value>
    </property>
    <property name="name">
      <value>Setting permissions</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/setting_permissions.htm</value>
    </property>
  </bean>
  <bean id="wikiContent9" class="org.sakaiproject.component.app.help.model.ResourceBean">
    <property name="docId">
      <value>atxq</value>
    </property>
    <property name="name">
      <value>Adding the Wiki tool to your site</value>
    </property>
    <property name="location">
      <value>/sakai_rwiki/atxq.html</value>
