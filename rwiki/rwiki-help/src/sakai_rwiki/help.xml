  </bean>
  <bean id="org.sakaiproject.api.app.help.TableOfContents" class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
    <property name="name">
      <value>root</value>
    </property>
    <property name="categories">
      <list>
        <bean id="wiki1" class="org.sakaiproject.component.app.help.model.CategoryBean">
          <property name="name">
            <value>Wiki</value>
          </property>
          <property name="resources">
            <list>
              <ref bean="wikiContent1"/>
              <ref bean="wikiContent2"/>
              <ref bean="wikiContent3"/>
              <ref bean="wikiContent4"/>
              <ref bean="wikiContent5"/>
              <ref bean="wikiContent6"/>
              <ref bean="wikiContent7"/>
              <ref bean="wikiContent8"/>
              <ref bean="wikiContent9"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>
