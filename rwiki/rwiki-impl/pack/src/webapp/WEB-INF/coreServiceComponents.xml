  </bean>

  <!-- The component level Render service -->
  <bean id="uk.ac.cam.caret.sakai.rwiki.service.api.RenderService"
    class="uk.ac.cam.caret.sakai.rwiki.component.service.impl.RenderServiceImpl"
    init-method="init" 
    >
  </bean>

