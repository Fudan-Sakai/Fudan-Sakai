       init-method="init" 
 	>
  </bean>

  <!-- Wiki engine factory -->
  <bean id="uk.ac.cam.caret.sakai.rwiki.service.api.radeox.RenderEngineFactory"
    class="uk.ac.cam.caret.sakai.rwiki.component.radeox.service.impl.RenderEngineFactoryImpl"
       init-method="init" 
