  <name>sakai-rwiki-tool</name>
  <groupId>org.sakaiproject</groupId>
  <artifactId>sakai-rwiki-tool</artifactId>
  <description>
    This project creates a WAR containing the RWiki Tool for Sakai.
  </description>
  <packaging>war</packaging>
  <build>
    <resources>
      <resource>
        <directory>${basedir}/src/bundle</directory>
        <includes>
          <include>**/*.*</include>
        </includes>
        <filtering>false</filtering>
      </resource>
      <resource>
        <directory>${basedir}/src/java</directory>
        <includes>
          <include>**/*.xml</include>
        </includes>
        <filtering>false</filtering>
      </resource>
    </resources>
<!--nagEmailAddress: andrew@caret.cam.ac.uk -->
<!-- java source -->
    <testResources>
      <testResource>
        <directory>${basedir}/src/testBundle</directory>
        <includes>
          <include>**/*.*</include>
        </includes>
        <filtering>false</filtering>
      </testResource>
<!--includes: **/*Test -->
    </testResources>
<!-- other resources for the jar - the message bundles -->
  </build>
  <dependencies>
    <dependency>
      <groupId>org.sakaiproject.kernel</groupId>
      <artifactId>sakai-kernel-util</artifactId>
    </dependency>
