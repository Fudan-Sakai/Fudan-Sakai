<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
   <bean id="siteInfo" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqym_ru</value></property>
      <property name="name"><value>Настройки: Обзор</value></property>
      <property name="location"><value>/sakai_siteinfo/aqym_ru.html</value></property>
      <property name="defaultForTool"><value>sakai.siteinfo</value></property>
   </bean>
   
   <bean id="editASite" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>araw_ru</value></property>
