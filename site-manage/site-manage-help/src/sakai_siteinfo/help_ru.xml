      <property name="name"><value>Управление блоками пользователей</value></property>
      <property name="location"><value>/sakai_siteinfo/atcs_ru.html</value></property>
   </bean>
   
   <bean id="grantGlobalAccess" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arci_ru</value></property>
      <property name="name"><value>Доступ к сайту</value></property>
      <property name="location"><value>/sakai_siteinfo/arci_ru.html</value></property>
   </bean>
   
   <bean id="addRemoveParticipant" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arav_ru</value></property>
      <property name="name"><value>Добавление и удаление участников</value></property>
      <property name="location"><value>/sakai_siteinfo/arav_ru.html</value></property>
   </bean>
   
   <bean id="permissionRoleOverview" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>araa_ru</value></property>
      <property name="name"><value>Просмотр списка участников</value></property>
      <property name="location"><value>/sakai_siteinfo/araa_ru.html</value></property>
   </bean>
   
   <bean id="participantRole" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>ardw_ru</value></property>
      <property name="name"><value>Добавление описания сайта</value></property>
      <property name="location"><value>/sakai_siteinfo/ardw_ru.html</value></property>
   </bean>
   
   <bean id="changeParticipantRole" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arfb_ru</value></property>
      <property name="name"><value>Изменение роли участников</value></property>
      <property name="location"><value>/sakai_siteinfo/arfb_ru.html</value></property>
   </bean>
   
   <bean id="publishSite" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arai_ru</value></property>
