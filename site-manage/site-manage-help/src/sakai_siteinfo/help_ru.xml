      <property name="location"><value>/sakai_siteinfo/ardu_ru.html</value></property>
   </bean>
   
   <bean id="importToSite" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arao_ru</value></property>
      <property name="name"><value>Импорт с другого сайта</value></property>
      <property name="location"><value>/sakai_siteinfo/arao_ru.html</value></property>
   </bean>
   
   <bean id="viewSiteInfo" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqzx_ru</value></property>
      <property name="name"><value>Просмотр информации о сайте</value></property>
      <property name="location"><value>/sakai_siteinfo/aqzx_ru.html</value></property>
   </bean>
   
