      <property name="location"><value>/sakai_siteinfo/arci_zh_TW.html</value></property>
   </bean>
   
   <bean id="addRemoveParticipant" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>arav_zh_TW</value></property>
      <property name="name"><value>新增／編輯／刪除參與者</value></property>
      <property name="location"><value>/sakai_siteinfo/arav_zh_TW.html</value></property>
   </bean>
   
   <bean id="permissionRoleOverview" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>araa_zh_TW</value></property>
      <property name="name"><value>檢視參與者</value></property>
      <property name="location"><value>/sakai_siteinfo/araa_zh_TW.html</value></property>
   </bean>
   
   <bean id="participantRole" class="org.sakaiproject.component.app.help.model.ResourceBean">
